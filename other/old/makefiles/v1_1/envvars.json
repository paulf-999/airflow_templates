{
  "airflow_args": {
    "airflow_home_dir": "~/airflow/",
    "airflow_version": "2.2.0",
    "constraints_url": "https://raw.githubusercontent.com/apache/airflow/constraints-${AIRFLOW_VERSION}/constraints-${PYTHON_VERSION}.txt",
    "python_version": "3.7"
  },
  "other": {
    "slack_token": "${MY_SLACK_TOKEN}"
  },
  "sample_db_creds": {
    "db_name": "${MY_DB}",
    "db_schema": "${MY_DB_SCHEMA}",
    "host": "${MY_HOST}",
    "ip_tbl_list": "${MY_IP_TBLS}",
    "password": "${MY_PWD}",
    "username": "${MY_USERNAME}"
  }
}
